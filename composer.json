{
    "name": "support-ticketing/system",
    "description": "Système de support ticketing sécurisé avec chiffrement",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "phpmailer/phpmailer": "^7.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
